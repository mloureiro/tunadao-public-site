{
  "name": "tunadao-cms",
  "version": "1.0.0",
  "description": "CMS for Tunad√£o 1998 website",
  "type": "module",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload serve",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "seed": "tsx src/seed/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@payloadcms/db-sqlite": "^3.0.0",
    "@payloadcms/richtext-lexical": "^3.0.0",
    "@payloadcms/email-resend": "^3.0.0",
    "payload": "^3.0.0",
    "cross-env": "^7.0.3",
    "express": "^4.21.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.2.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.8.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
